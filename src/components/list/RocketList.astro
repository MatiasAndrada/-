---
import RocketCard from "../cards/RocketCard.astro";
import { getRockets } from "../../services/api";
const list = await getRockets();
---

<ul class="mt-4 launch-list flex gap-6 justify-evenly flex-wrap">
  {
    list.map((rocket) => (
      <RocketCard
        id={rocket.id}
        name={rocket.name}
        img={rocket.flickr_images[0]}
        first_flight={rocket.first_flight}
        active={rocket.active}
      />
    ))
  }
</ul>
